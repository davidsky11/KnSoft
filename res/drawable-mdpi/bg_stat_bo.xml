<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#ffebebeb" />
            <stroke android:width="1.0px" android:color="#ffbbbbbb" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffb2b2b2" android:endColor="#ffd1d1d1" android:angle="90.0" />
            <stroke android:width="1.0px" android:color="#ffbbbbbb" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ffd1d1d1" android:endColor="#fff6f6f6" android:angle="90.0" />
            <stroke android:width="1.0px" android:color="#ffbbbbbb" />
        </shape>
    </item>
</selector>